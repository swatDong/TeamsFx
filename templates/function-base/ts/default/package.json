{
  "name": "teamsfx-template-api",
  "version": "1.0.0",
  "scripts": {
    "/* Teams Toolkit uses dev:teamsfx for local debug (https://aka.ms/teamsfx-localdebug) */": "",
    "dev:teamsfx": "env-cmd --silent -f .env.teamsfx.local npm run dev",
    "dev": "npx func start --typescript --language-worker=\"--inspect=9229\" --port \"7071\" --cors \"*\"",
    "watch:teamsfx": "tsc --watch",
    "build": "tsc",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "start": "npx func start"
  },
  "dependencies": {
    "@azure/functions": "^1.2.2",
    "@microsoft/microsoft-graph-client": "^2.2.1",
    "@microsoft/teamsfx": "^0.2.0",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "typescript": "^3.3.3"
  }
}
